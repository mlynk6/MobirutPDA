<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#df121312" />
            <stroke android:width="1dp" android:color="#00101c78" />
            <corners android:radius="6dp" />
            <padding android:left="10dp" android:top="10dp" android:right="10dp" android:bottom="10dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#c21a1818" android:endColor="#d22e2d2d" android:angle="270" />
            <stroke android:width="1dp" android:color="#c42f302f" />
            <corners android:radius="6dp" />
            <padding android:left="10dp" android:top="10dp" android:right="10dp" android:bottom="10dp" />
        </shape>
    </item>
</selector>